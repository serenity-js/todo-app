    "@serenity-js/mocha": "^2.19.10",
    "@serenity-js/protractor": "^2.19.10",
    "@serenity-js/rest": "^2.19.10",
    "@serenity-js/serenity-bdd": "^3.1.4",
    "@types/chai": "^4.2.14",
